{
  "realm": "school",
  "enabled": true,
  "registrationAllowed": false,
  "loginTheme": "keycloak",
  "clients": [
    {
      "clientId": "outline",
      "name": "Outline",
      "enabled": true,
      "publicClient": false,
      "protocol": "openid-connect",
      "redirectUris": [
        "https://docs.school.edu/auth/oidc.callback"
      ],
      "webOrigins": [
        "https://docs.school.edu"
      ],
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "secret": "CHANGE_ME_OIDC_CLIENT_SECRET"
    }
  ],
  "groups": [
    { "name": "Staff" },
    { "name": "staff-tech" },
    { "name": "staff-hr" },
    { "name": "admin" }
  ],
  "identityProviders": [
    {
      "alias": "wecom",
      "displayName": "WeCom",
      "providerId": "oidc",
      "enabled": true,
      "storeToken": true,
      "addReadTokenRoleOnCreate": false,
      "authenticateByDefault": false,
      "linkOnly": false,
      "trustEmail": true,
      "config": {
        "clientId": "CHANGE_ME_WECOM_CLIENT_ID",
        "clientSecret": "CHANGE_ME_WECOM_CLIENT_SECRET",
        "authorizationUrl": "CHANGE_ME_WECOM_AUTH_URL",
        "tokenUrl": "CHANGE_ME_WECOM_TOKEN_URL",
        "userInfoUrl": "CHANGE_ME_WECOM_USERINFO_URL",
        "defaultScope": "openid profile email",
        "uiLocales": "en",
        "backchannelSupported": "true",
        "jwksUrl": "CHANGE_ME_WECOM_JWKS_URL"
      }
    }
  ],
  "protocolMappers": [
    {
      "name": "groups",
      "protocol": "openid-connect",
      "protocolMapper": "oidc-group-membership-mapper",
      "consentRequired": false,
      "config": {
        "full.path": "false",
        "id.token.claim": "true",
        "access.token.claim": "true",
        "userinfo.token.claim": "true",
        "claim.name": "groups"
      }
    }
  ]
}
