{
  "realm": "school",
  "enabled": true,
  "registrationAllowed": false,
  "loginTheme": "keycloak",
  "sslRequired": "none",
  "clients": [
    {
      "clientId": "outline",
      "name": "Outline",
      "enabled": true,
      "publicClient": false,
      "protocol": "openid-connect",
      "redirectUris": [
        "https://docs.school.edu/auth/oidc.callback",
        "http://docs.localhost/auth/oidc.callback"
      ],
      "webOrigins": [
        "https://docs.school.edu",
        "http://docs.localhost"
      ],
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "attributes": {},
      "secret": "CHANGE_ME_OIDC_CLIENT_SECRET"
    }
  ],
  "groups": [
    { "name": "Staff" },
    { "name": "staff-tech" },
    { "name": "staff-hr" },
    { "name": "admin" }
  ],
  "identityProviders": [],
  "protocolMappers": [
    {
      "name": "groups",
      "protocol": "openid-connect",
      "protocolMapper": "oidc-group-membership-mapper",
      "consentRequired": false,
      "config": {
        "full.path": "false",
        "id.token.claim": "true",
        "access.token.claim": "true",
        "userinfo.token.claim": "true",
        "claim.name": "groups"
      }
    }
  ]
}
