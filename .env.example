# Domains
DOCS_DOMAIN=docs.school.edu
AUTH_DOMAIN=auth.school.edu
FLOW_DOMAIN=flow.school.edu
PUBLIC_URL_SCHEME=https
ENABLE_TLS=true

# Postgres superuser (container-level)
POSTGRES_SUPERUSER=postgres
POSTGRES_SUPERPASS=change_me_super

# Outline DB user password
OUTLINE_DB_PASSWORD=change_me_outline

# Keycloak DB user password
KEYCLOAK_DB_PASSWORD=change_me_keycloak

# n8n DB user password
N8N_DB_PASSWORD=change_me_n8n

# Outline runtime secrets (generate random hex)
OUTLINE_SECRET_KEY=
OUTLINE_UTILS_SECRET=

# OIDC (Keycloak) client for Outline (configure in Keycloak)
OIDC_CLIENT_ID=outline
OIDC_CLIENT_SECRET=change_me_oidc_secret
KEYCLOAK_REALM=school

# Object storage (Tencent COS in prod, MinIO for dev)
# For COS, set region and bucket URL accordingly, for example:
# AWS_REGION=ap-shanghai
# AWS_S3_UPLOAD_BUCKET_URL=https://cos.ap-shanghai.myqcloud.com
# For MinIO, use http://minio:9000 and force path style via bucket URL
AWS_REGION=
AWS_S3_UPLOAD_BUCKET_URL=
AWS_S3_UPLOAD_BUCKET_NAME=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_FORCE_PATH_STYLE=

# Optional SMTP (leave empty to disable)
SMTP_HOST=
SMTP_PORT=
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_FROM_EMAIL=

# n8n config
N8N_ENCRYPTION_KEY=change_me_n8n_key
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change_me

# Outline API token for n8n test workflow
OUTLINE_API_TOKEN=

# MinIO defaults (only for local/dev)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# Local dev example (uncomment for local testing)
# DOCS_DOMAIN=docs.localhost
# AUTH_DOMAIN=auth.localhost
# FLOW_DOMAIN=flow.localhost
# PUBLIC_URL_SCHEME=http
# ENABLE_TLS=false

# WeCom (Keycloak IdP) placeholders
WECOM_CLIENT_ID=
WECOM_CLIENT_SECRET=
WECOM_AUTH_URL=
WECOM_TOKEN_URL=
WECOM_USERINFO_URL=
WECOM_JWKS_URL=
